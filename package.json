        "doc": "cd server && typedoc --out ../docs ../server",
        "fix": "gts fix",
        "lint": "eslint \"./**/*.{ts,tsx}\"",
        "map": "cd tools/map && ts-node exportmap data/map-refactor.json",
        "setup": "mv ./client/data/config.json-dist ./client/data/config.json && mv ./server/config.dev.ts ./server/config.ts",
        "start": "node dist/ts/main",
        "test": "jest"
    },
